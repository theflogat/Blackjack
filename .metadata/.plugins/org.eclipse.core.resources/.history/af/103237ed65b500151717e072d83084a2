package isn.cards;

public class Card {
	
	public enum Type{
		SPADE("Pique"),
		HEART("Coeur"),
		DIAMONDS("Carreau"),
		CLUBS("Tr√®fle");
		
		String name;
		
		private Type(String name) {
			this.name = name;
		}
	}
	
	int number;
	Type type;
	
	public Card(int number, Type type) throws Exception {
		if(number<0 || number>12){
			throw new Exception();
		}
		
		this.number = number;
		this.type = type;
	}
	
	
	
}
