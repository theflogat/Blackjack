package isn.cards;

public class Card {
	
	public enum Type{
		SPADE("Pique"),
		HEART("Coeur"),
		DIAMONDS("Carreau"),
		CLUBS("Tr√®fle");
		
		String name;
		
		private Type(String name) {
			this.name = name;
		}
	}
	
	int number;
	int type;
	
	public Card(int number, Type type) {
		this.number = number;
		this.type = type;
	}
	
}
